# `brew install coreutils` installs ls, cat, etc., but prepends a g.
# This ensures normal ls does the right thing.
export PATH="/opt/homebrew/bin:$PATH"
export PATH="/usr/local/opt/coreutils/libexec/gnubin:$PATH"
export PATH="/Users/joshua/anaconda3/bin:$PATH"
export PATH="/Library/Frameworks/GDAL.framework/Programs:$PATH"

export PATH="/usr/local/opt/ruby/bin:$PATH"
export PATH="/usr/local/Cellar/ruby/3.0.0_1/bin/bundle:$PATH"
export LDFLAGS="-L/usr/local/opt/ruby/lib"
export CPPFLAGS="-I/usr/local/opt/ruby/include"
export PKG_CONFIG_PATH="/usr/local/opt/ruby/lib/pkgconfig"

# Add `code` to path (VSCode)
export PATH="$PATH:/Applications/Visual Studio Code.app/Contents/Resources/app/bin"
# Add `docker` to path
export PATH="$PATH:/Applications/Docker.app/Contents/Resources/bin/"

export PATH="$PATH:/Users/joshua/Library/Python/3.9/bin/"
